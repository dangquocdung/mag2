    "extra": {
        "laravel": {
            "providers": [
                "Botble\\Base\\Providers\\BaseServiceProvider",
                "Botble\\Base\\Providers\\CommandServiceProvider",
                "Botble\\Base\\Providers\\EventServiceProvider",
