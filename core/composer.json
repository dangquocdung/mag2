                "ChartBuilder": "Botble\\Base\\Facades\\ChartBuilderFacade",
                "MailVariable": "Botble\\Base\\Facades\\MailVariableFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
