        {
            "name": "Sang Nguyen",
            "email": "sangnguyenplus@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "barryvdh/laravel-dompdf": "^0.8.2",
        "chumper/zipper": "^1.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.1",
        "doctrine/dbal": "^2.6",
        "intervention/image": "^2.4",
        "ixudra/curl": "^6.16",
        "jeremeamia/SuperClosure": "^2.4",
        "kris/laravel-form-builder": "^1.14",
        "laravel/passport": "^7.0",
